    <panel>
      <title>Correlation Id Tracking Dashboard</title>
      <viz type="network-diagram-viz.network-diagram-viz">
        <search>
          <query>$correlationId$
| rex field=_raw "application\s*=\s*(?&lt;application&gt;[^\s]+)"
| rex field=_raw "correlationId\s*=\s*(?&lt;correlationId&gt;[^\s]+)"
| table _time correlationId application
| sort correlationId _time
| streamstats current=f last(application) as from by correlationId
| rename application as to
| where from != to OR isnull(from)
| table correlationId _time from to</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
